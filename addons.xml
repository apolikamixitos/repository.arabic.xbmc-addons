<?xml version="1.0" encoding="UTF-8"?>
<addons>

<addon id="plugin.audio.radioma" name="RadioMA" version="1.0" provider-name="Ayoub DARDORY (Apolikamixitos)">
	<requires>
		<import addon="xbmc.python" version="1.0"/>
		<import addon="script.module.beautifulsoup" version="3.0.8"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
		<import addon="script.module.xbmcswift" version="0.1.3"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>audio</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary>Listen to the Moroccan radio stations online.</summary>
		<description>With this plugin you'll be able to listen to the all Moroccan radio stations that are broadcasting online.
		</description>
	</extension>
</addon>

